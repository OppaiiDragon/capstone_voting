# Vercel Frontend Environment Variables
# Replace with your actual Railway backend URL
VITE_API_BASE_URL=https://your-railway-backend-url.railway.app/api
VITE_API_VERSION=v1
VITE_API_TIMEOUT=10000

# Cloudinary Configuration (for image uploads)
VITE_CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
VITE_CLOUDINARY_UPLOAD_PRESET=your-upload-preset
VITE_CLOUDINARY_API_KEY=your-api-key

# Vercel Blob Configuration (alternative to Cloudinary)
VITE_BLOB_READ_WRITE_TOKEN=vercel_blob_rw_your_token_here

# Frontend Configuration
VITE_APP_NAME=Voting System
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Secure Online Voting System

# Environment
VITE_NODE_ENV=development
VITE_APP_ENV=development

# Authentication Configuration
VITE_JWT_STORAGE_KEY=voting_system_token
VITE_REFRESH_TOKEN_KEY=voting_system_refresh_token
VITE_TOKEN_EXPIRY_CHECK_INTERVAL=60000

# File Upload Configuration
VITE_MAX_FILE_SIZE=5242880
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif
VITE_UPLOAD_ENDPOINT=/api/upload

# UI Configuration
VITE_THEME=light
VITE_LANGUAGE=en
VITE_TIMEZONE=UTC
VITE_DATE_FORMAT=YYYY-MM-DD
VITE_TIME_FORMAT=HH:mm:ss

# Feature Flags
VITE_ENABLE_REGISTRATION=true
VITE_ENABLE_PASSWORD_RESET=true
VITE_ENABLE_FILE_UPLOAD=true
VITE_ENABLE_REAL_TIME_UPDATES=false
VITE_ENABLE_ANALYTICS=false

# Analytics Configuration (if enabled)
VITE_ANALYTICS_ID=
VITE_ANALYTICS_ENABLED=false

# Real-time Configuration (if enabled)
VITE_WEBSOCKET_URL=ws://localhost:3000
VITE_WEBSOCKET_ENABLED=false

# External Services
VITE_EMAIL_SERVICE_ENABLED=false
VITE_SMS_SERVICE_ENABLED=false

# Security Configuration
VITE_CSP_ENABLED=true
VITE_XSS_PROTECTION=true
VITE_CONTENT_TYPE_OPTIONS=true

# Performance Configuration
VITE_ENABLE_SERVICE_WORKER=false
VITE_ENABLE_PWA=false
VITE_CACHE_STRATEGY=network-first

# Development Configuration
VITE_DEV_SERVER_PORT=5173
VITE_DEV_SERVER_HOST=localhost
VITE_HOT_RELOAD=true

# Production Configuration
VITE_PROD_API_URL=https://your-api-domain.com/api
VITE_PROD_FRONTEND_URL=https://your-frontend-domain.com

# Build Configuration
VITE_BUILD_OUTPUT_DIR=dist
VITE_BUILD_SOURCEMAP=true
VITE_BUILD_MINIFY=true

# Testing Configuration
VITE_TEST_ENABLED=false
VITE_TEST_COVERAGE_ENABLED=false

# Monitoring Configuration
VITE_ERROR_TRACKING_ENABLED=false
VITE_PERFORMANCE_MONITORING_ENABLED=false

# Localization Configuration
VITE_DEFAULT_LOCALE=en
VITE_FALLBACK_LOCALE=en
VITE_AVAILABLE_LOCALES=en,es,fr

# Cloudinary Configuration
VITE_CLOUDINARY_CLOUD_NAME=your_cloud_name
VITE_CLOUDINARY_UPLOAD_PRESET=your_upload_preset
VITE_CLOUDINARY_API_KEY=your_api_key

# Custom Configuration
VITE_CUSTOM_THEME_COLORS=
VITE_CUSTOM_LOGO_URL=
VITE_CUSTOM_FAVICON_URL= 